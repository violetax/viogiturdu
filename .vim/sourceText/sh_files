#!/bin/bash
#wis

## SOURCES 
##VARS-FILES
source Asksure.fu
source Opte.fu
source Continua.fu
source Echocolors.fu # black red green yellow blue magenta cyan white pink orange whiteguay @+N
##FUNCTIONS
##DIRECTORIES 
##FILES
#VARIABLES

while getopts ":eh" o; do
	case ${o} in
		e)
			opte
			exit
			;;
		h)
			whatis 2>/dev/null
			[[ "$?" == 25  ]] || echo -e "PENDING whatis!! ðŸ”¥ "
			;; 
		\?)
			echo -e "Nein nein nein option â†’ ${o}"
			exit
			;;
	esac
    
done
shift $((OPTIND-1))

### MAIN

.
