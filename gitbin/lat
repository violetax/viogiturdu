function waiting() {
	echo -e "\e[38;5;195m" 
	while read -r -p "$@ " -n 1 -s answer
	do
		if [[ ${answer} == [gG] ]]; then
			break
		fi
	done
	clomanow -v
}
function espiask() {
	echo -e "espidi??"
	if asksure; then
		espidi
	fi
}
function latsayshello() { #latsayshello
	# 1/ check if in PROYECTO and get lat if there is one
	if [[ ${curdir} =~ "PROYECTO" ]]; then  
		symfs=$(find -L -xtype l)
		haylat=$(echo ${symfs} | sed -ne "s/.*\(lat\).*/\1/p")
		#haylat=$(echo ${symfs} | grep -wo lat )
	else
		echo -e "ğŸ”¥\e[38;5;11mnot in \e[38;5;118mPâ­Râ­Oâ­Yâ­Eâ­Câ­Tâ­O \e[38;5;212mbaby ğŸ’¥ ğŸ’¥ ğŸ’¥ "
		exit
	fi	

	# 2/ if in PROYECTO, either show or link lat
	if [[ ${haylat} == ""  ]]; then
		echo -e "\e[38;5;9mNO haylat \e[38;5;118m â†’ pues se linka "
		echo -e "\e[38;5;195mlat script: ${latf}" 	
		ln -s ${latf} 
	else
		echo -e "\e[38;5;14mSÃ haylat â†’ \e[38;5;195m${haylat[0]} "
		echo -e "\e[38;5;228mâ­ \`$1\` \e[38;5;212msays âš¡HELLOâš¡âš¡ \e[38;5;195m:D!!!!!!!!!!ğŸ’•"  
	fi
	echo
}

