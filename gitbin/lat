	cuseco=$(basename $0)
    echo -e "\\e[38;5;118m${cuseco}	â†’ \\e[38;5;11mtype:\\e[38;5;118mvio-bas admin: alias, ln, bin, PROYECTO"
	echo -e "\\e[38;5;118m â‡’  labour.today\\\e[38;5;195mun s.sh del dÃ­a \\e[38;5;14msolo funciona within PROYECTO \\e[38;5;11mâ­\\e[38;5;195mtipo fastcd, pero para run la script del day (o del momento) \\e[38;5;11mâ­\\e[38;5;195m \\e[38;5;212mâ¤ï¸ï¸ \\n\\e[38;5;43m\tğŸ‘‰\t\e[38;5;11m${cuseco} -$o\e[0m"
