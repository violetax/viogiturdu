#!/bin/bash
function whatis() { #whatis
	cuseco=$(basename $0)
    echo -e "\\033[38;5;118m${cuseco}	‚Üí \\033[38;5;11mtype:\\033[38;5;118mvio-bas admin: alias, ln, bin, PROYECTO"
	echo -e "\\033[38;5;118m ‚áí  labour.today\\\033[38;5;195mun s.sh del d√≠a \\033[38;5;14msolo funciona within PROYECTO \\033[38;5;11m‚≠ê\\033[38;5;195mtipo fastcd, pero para run la script del day (o del momento) \\033[38;5;11m‚≠ê\\033[38;5;195m \\033[38;5;212m‚ù§Ô∏èÔ∏è \\n\\033[38;5;43m\tüëâ\t\033[38;5;11m${cuseco} -$o\033[0m"
}

## SOURCES
##FUNCTIONS
source Opte.fu
source Asksure.fu
source Continua.fu

function waiting() {
	echo -e "\033[38;5;195m" 
	while read -r -p "$@ " -n 1 -s answer
	do
		if [[ ${answer} == [gG] ]]; then
			break
		fi
	done
	clomanow -v
}
function espiask() {
	echo -e "espidi??"
	if asksure; then
		espidi
	fi
}
function latsayshello() { #latsayshello
	# 1/ check if in PROYECTO and get lat if there is one
	if [[ ${curdir} =~ "PROYECTO" ]]; then  
		symfs=$(find -L -xtype l)
		haylat=$(echo ${symfs} | sed -ne "s/.*\(lat\).*/\1/p")
		#haylat=$(echo ${symfs} | grep -wo lat )
	else
		echo -e "üî•\033[38;5;11mnot in \033[38;5;118mP‚≠êR‚≠êO‚≠êY‚≠êE‚≠êC‚≠êT‚≠êO \033[38;5;212mbaby üí• üí• üí• "
		exit
	fi	

	# 2/ if in PROYECTO, either show or link lat
	if [[ ${haylat} == ""  ]]; then
		echo -e "\033[38;5;9mNO haylat \033[38;5;118m ‚Üí pues se linka "
		echo -e "\033[38;5;195mlat script: ${latf}" 	
		ln -s ${latf} 
	else
		echo -e "\033[38;5;14mS√ç haylat ‚Üí \033[38;5;195m${haylat[0]} "
		echo -e "\033[38;5;228m‚≠ê \`$1\` \033[38;5;212msays ‚ö°HELLO‚ö°‚ö° \033[38;5;195m:D!!!!!!!!!!üíï"  
	fi
	echo
}

